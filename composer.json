{
    "name": "iqomp/migrate",
    "description": "Database migration that sync migrate config to database table",
    "type": "library",
    "license": "MIT",
    "version": "3.0.1",
    "authors": [
        {
            "name": "Iqbal Fauzi",
            "email": "iqbalfawz@gmail.com"
        }
    ],
    "require": {
        "iqomp/model": "^2.0",
        "hyperf/command": "^2.1"
    },
    "extra": {
        "hyperf": {
            "config": "Iqomp\\Migrate\\ConfigProvider"
        }
    },
    "autoload": {
        "psr-4": {
            "Iqomp\\Migrate\\": "src/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*"
    },
    "scripts": {
        "lint": "./vendor/bin/phpcs --standard=PSR12 src"
    },
    "suggest": {
        "iqomp/migrate-mysql": "Database mysql migration driver"
    }
}
